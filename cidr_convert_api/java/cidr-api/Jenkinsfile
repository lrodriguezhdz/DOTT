pipeline {
    agent any
       tools { 
        maven 'Maven' 
    }
    stages {
        stage('Build 2') { 
            steps {
                sh 'pwd'
                sh 'mvn -f cidr_convert_api/java/cidr-api/pom.xml clean install -e'
                sh 'mvn -B -DskipTests clean package' 
            }
        }
    }
}